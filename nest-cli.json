{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/client-gatway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/client-gatway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/client-gatway",
  "projects": {
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "catalog-service": {
      "type": "application",
      "root": "apps/catalog-service",
      "entryFile": "main",
      "sourceRoot": "apps/catalog-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/catalog-service/tsconfig.app.json"
      }
    },
    "client-gateway": {
      "type": "application",
      "root": "apps/client-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/client-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/client-gateway/tsconfig.app.json"
      }
    },
    "client-gatway": {
      "type": "application",
      "root": "apps/client-gatway",
      "entryFile": "main",
      "sourceRoot": "apps/client-gatway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/client-gatway/tsconfig.app.json"
      }
    },
    "notification-service": {
      "type": "application",
      "root": "apps/notification-service",
      "entryFile": "main",
      "sourceRoot": "apps/notification-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notification-service/tsconfig.app.json"
      }
    },
    "order-service": {
      "type": "application",
      "root": "apps/order-service",
      "entryFile": "main",
      "sourceRoot": "apps/order-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/order-service/tsconfig.app.json"
      }
    },
    "payment-service": {
      "type": "application",
      "root": "apps/payment-service",
      "entryFile": "main",
      "sourceRoot": "apps/payment-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/payment-service/tsconfig.app.json"
      }
    },
    "product-service": {
      "type": "application",
      "root": "apps/product-service",
      "entryFile": "main",
      "sourceRoot": "apps/product-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/product-service/tsconfig.app.json"
      }
    },
    "pyment-service": {
      "type": "application",
      "root": "apps/pyment-service",
      "entryFile": "main",
      "sourceRoot": "apps/pyment-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/pyment-service/tsconfig.app.json"
      }
    }
  }
}